<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>b974757e-4b5b-499d-84c1-efa5a41c8fac</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="LCMC"/>
  <Description DefaultValue="LCMC Task Manager Add-in"/>
  <IconUrl DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/assets/icon-botlcmc2.png"/>
  <HighResolutionIconUrl DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/assets/icon-botlcmc2.png"/>
  <SupportUrl DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/"/>
  <AppDomains>
    <AppDomain>https://d16qufmbo7j8d8.cloudfront.net</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.botlcmc"/>
                  <bt:Image size="32" resid="Icon.botlcmc"/>
                  <bt:Image size="80" resid="Icon.botlcmc"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.botlcmc"/>
                    <bt:Image size="32" resid="Icon.botlcmc"/>
                    <bt:Image size="80" resid="Icon.botlcmc"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.botlcmc" DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/assets/icon-botlcmc2.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://d16qufmbo7j8d8.cloudfront.net/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="LCMC"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show LCMC Task Manager"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded successfully. Go to the HOME tab and click the 'Show Task Pane' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Show LCMC Task Manager Pane"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>